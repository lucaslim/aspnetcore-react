{
  "name": "aspnetcore-react",
  "version": "1.0.0",
  "description": "Starter kit for .Net core with React",
  "main": "index.js",
  "repository": "git@github.com:lucaslim/aspnetcore-react.git",
  "author": "Lucas L. <hi@lucas.codes>",
  "license": "MIT",
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10",
    "yarn": "0.22.0"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./server/wwwroot/dist",
    "restore": "dotnet restore && dotnet build",
    "start": "cross-env BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=development ASPNETCORE_ENVIRONMENT=Development dotnet run",
    "build:dev": "cross-env BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=development ASPNETCORE_ENVIRONMENT=Development webpack --progress --hide-modules",
    "build:prod": "cross-env BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=production ASPNETCORE_ENVIRONMENT=Production webpack --progress --hide-modules",
    "publish-iis": "npm run clean && dotnet publish -c Release",
    "pack": "npm run clean && dotnet pack -c Release",
    "test": "cross-env BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=test karma start"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "redbox-react": "^1.3.6",
    "redux-logger": "^3.0.1",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "universal-style-loader": "^0.16.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "aspnet-prerendering": "^2.0.5",
    "aspnet-webpack": "^1.0.29",
    "aspnet-webpack-react": "^1.0.5",
    "babel-polyfill": "^6.23.0",
    "history": "^4.6.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
